{
  "name": "boilerplate-react-webpack",
  "version": "0.2.3",
  "description": "A webpack React starter",
  "repository": "https://github.com/dev-academy-challenges/boilerplate-react-webpack.git",
  "main": "server/index.js",
  "author": "EDA",
  "license": "ISC",
  "scripts": {
    "client": "webpack serve --config ./client/webpack.config.js --hot",
    "dev": "run-p dev:client dev:server",
    "dev:client": "npm run webpack -- --watch",
    "dev:server": "nodemon server",
    "start": "node server",
    "build": "npm run webpack",
    "server": "node server",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:e2e": "NODE_ENV:e2e PORT:3001 server-test 3001 e2e",
    "test:e2e:debug": "DEBUG=pw:api npm run test:e2e",
    "e2e": "jest e2e --config e2e/jest.config.js --verbose",
    "webpack": "webpack --config ./client/webpack.config.js",
    "lint": "eslint --ext .js,.jsx .",
    "tailbuild": "tailwindcss build src/main.css -o server/public/styles/styles.css",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "db:reset": "npm run knex migrate:rollback && npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "fullstack"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.13.9",
    "@googlemaps/google-maps-services-js": "^3.1.16",
    "@tailwindcss/forms": "^0.2.1",
    "authenticare": "^0.4.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "google-map-react": "^2.1.9",
    "knex": "^0.21.18",
    "mathjs": "^9.2.0",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.2",
    "@tailwindcss/custom-forms": "^0.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.2",
    "eslint": "7.9.0",
    "eslint-config-eda": "0.1.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.9.1",
    "postcss": "^8.2.6",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^5.0.0",
    "precss": "^4.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "style-loader": "^2.0.0",
    "superagent": "^6.1.0",
    "supertest": "^6.1.3",
    "tailwindcss": "^2.0.3",
    "testing-library": "0.0.2",
    "uuid": "^8.3.2",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  }
}
